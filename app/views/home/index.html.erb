<div class="wrapper" ng-app="edzoApp">
  <div class="tools" ng-controller="toolsCtrl">
    <button class=""></button>
  </div>
  <div class="main">
    <div class="graph" ng-controller="graphCtrl">
      <div class="node" ng-repeat="node in nodes" ng-controller="nodeCtrl" style="top: {{node.position.y * 100}}px; left: {{node.position.x * 100}}px" draggable>
        {{node.person.name}}
      </div>
    </div>
  </div>
</div>
